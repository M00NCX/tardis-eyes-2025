datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Annotation {
  id            String   @id @default(cuid())
  lat           Float
  lng           Float
  title         String
  description   String
  author        String
  created_at    DateTime @default(now())
  planet        String
  is_historical Boolean  @default(false)

  @@index([planet])
}
